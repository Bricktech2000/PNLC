# vim:tw=80:ft=pnlc:
# inc:../prelude.pnlc

..def ..cons '?'u8 ..cons '\s'u8 nil \"?\s"
..def ..cons '>'u8 ..cons '\s'u8 nil \">\s"

.main .fix \main
  ...bindk io:monad .puts "?\s"
    ...bind io:monad getln \str
      ...bindk io:monad .puts ">\s"
        ...bindk io:monad .puts str
          main
